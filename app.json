{
  "expo": {
    "name": "Game Shop",
    "slug": "video-game-ecommerce",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/app-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1d1e21"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-logo.png",
        "backgroundColor": "#1d1e21"
      },
      "package": "com.ramirolandajo.videogameecommerce"
    },
    "web": {
      "favicon": "./assets/app-logo.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "babbd293-9442-4d6a-a9ba-94b513df80bc"
      }
    }
  }
}
